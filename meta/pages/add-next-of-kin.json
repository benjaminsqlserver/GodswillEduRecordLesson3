{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Student Biodatum",
                              "component": "BiodataID",
                              "name": "BiodataIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getStudentBiodataResult}",
                              "textProperty": "Surname",
                              "valueProperty": "BiodataID",
                              "placeholder": "Choose StudentBiodatum",
                              "valueType": "Int64",
                              "type": "dropdown",
                              "name": "BiodataID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${nextofkin.BiodataID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "BiodataIDRequiredValidator",
                              "component": "BiodataID",
                              "text": "BiodataID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name Of Next Of Kin",
                              "component": "NameOfNextOfKin",
                              "name": "NameOfNextOfKinLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "NameOfNextOfKin",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${nextofkin.NameOfNextOfKin}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameOfNextOfKinRequiredValidator",
                              "component": "NameOfNextOfKin",
                              "text": "NameOfNextOfKin is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Relationship",
                              "component": "RelationshipID",
                              "name": "RelationshipIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getRelationshipsResult}",
                              "textProperty": "RelationshipName",
                              "valueProperty": "RelationshipID",
                              "placeholder": "Choose Relationship",
                              "valueType": "int",
                              "type": "dropdown",
                              "name": "RelationshipID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${nextofkin.RelationshipID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "RelationshipIDRequiredValidator",
                              "component": "RelationshipID",
                              "text": "RelationshipID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Next Of Kin Phone No",
                              "component": "NextOfKinPhoneNo",
                              "name": "NextOfKinPhoneNoLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "NextOfKinPhoneNo",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${nextofkin.NextOfKinPhoneNo}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NextOfKinPhoneNoRequiredValidator",
                              "component": "NextOfKinPhoneNo",
                              "text": "NextOfKinPhoneNo is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Next Of Kin Email",
                              "component": "NextOfKinEmail",
                              "name": "NextOfKinEmailLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "NextOfKinEmail",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${nextofkin.NextOfKinEmail}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NextOfKinEmailRequiredValidator",
                              "component": "NextOfKinEmail",
                              "text": "NextOfKinEmail is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Next Of Kin Contact Address",
                              "component": "NextOfKinContactAddress",
                              "name": "NextOfKinContactAddressLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "NextOfKinContactAddress",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${nextofkin.NextOfKinContactAddress}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NextOfKinContactAddressRequiredValidator",
                              "component": "NextOfKinContactAddress",
                              "text": "NextOfKinContactAddress is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new NextOfKin!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ConData.createNextOfKin",
                        "parameters": [
                          {
                            "name": "NextOfKin",
                            "value": "${nextofkin}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${nextofkin}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${nextofkin}",
                  "visible": "${nextofkin != null}",
                  "itemType": "GodswillEduRecord.Models.ConData.NextOfKin"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ConData.getStudentBiodata",
        "parameters": [],
        "then": [
          {
            "name": "getStudentBiodataResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<GodswillEduRecord.Models.ConData.StudentBiodatum>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "ConData.getRelationships",
        "parameters": [],
        "then": [
          {
            "name": "getRelationshipsResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<GodswillEduRecord.Models.ConData.Relationship>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "nextofkin",
        "type": "set",
        "value": "new GodswillEduRecord.Models.ConData.NextOfKin(){}",
        "valueType": "GodswillEduRecord.Models.ConData.NextOfKin"
      }
    ]
  },
  "name": "Add Next Of Kin",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}