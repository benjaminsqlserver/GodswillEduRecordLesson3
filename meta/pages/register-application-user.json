{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "userName",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Email",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "userName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${user.Name}"
                            },
                            {
                              "component": "userName",
                              "name": "EmailRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "Email is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "Password",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Password",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "password",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "password",
                              "value": "${user.Password}"
                            },
                            {
                              "component": "Password",
                              "name": "PasswordRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "Password is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "ConfirmPassword",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Confirm password",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "ConfirmPassword",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "password",
                              "value": "${user.ConfirmPassword}"
                            },
                            {
                              "component": "ConfirmPassword",
                              "name": "ConfirmPasswordRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "ConfirmPassword is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Register",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "back"
                                  }
                                ]
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "method": "post",
                  "action": "account/register",
                  "data": "${user}",
                  "name": "form0",
                  "type": "template-form",
                  "itemType": "ApplicationUser",
                  "visible": "${user != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "user",
        "type": "set",
        "value": "new ApplicationUser();",
        "valueType": "ApplicationUser"
      }
    ]
  },
  "layout": "Main",
  "name": "Register Application User"
}